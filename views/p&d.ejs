<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Samarpan</title>
    <link rel="icon" href="./Pics/top-logo.jpeg.jpg" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
    />
    <link rel="stylesheet" href="/css/p&d.css" />
  </head>
  <body>
    <%- include("includes/navbar.ejs"); %>

    <div class="main-body">
      <h1>Products & Districts</h1>

      <h3>Jammu Division</h3>
      <div class="row-container">
        <% for(district of jammu) { %>

        <div onclick="location.href='/<%= district.name %>'" class="rows">
          <h4><%= district.name %></h4>
          <p>Product:-</p>
        </div>

        <% } %>
      </div>

      <h3>Kashmir Division</h3>
      <div class="row-container">
        <% for(district of kashmir) { %>

        <div onclick="location.href='/<%= district.name %>'" class="rows">
          <h4><%= district.name %></h4>
          <p>Product:-</p>
        </div>

        <% } %>
      </div>
    </div>

    <%- include("includes/footer.ejs"); %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
